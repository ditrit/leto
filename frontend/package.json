{
	"name": "leto-quasar",
	"version": "0.0.1",
	"description": "User interface for the Leto project based on Vuejs/Quasar",
	"productName": "Leto",
	"author": "BS",
	"private": true,
	"scripts": {
		"lint": "eslint --ext .js,.vue ./",
		"dev": "quasar dev",
		"build": "quasar build",
		"sonar": "node_modules/sonar-scanner/bin/sonar-scanner",
		"start": "quasar serve",
		"build:pwa": "quasar build -m pwa",
		"test:unit:ci": "jest --ci",
		"test:unit:coverage": "jest --coverage",
		"test:unit:watch": "jest --watch",
		"test:unit:watchAll": "jest --watchAll",
		"serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
		"concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
	},
	"dependencies": {
		"@intlify/vue-i18n-loader": "^3.3.0",
		"@quasar/extras": "^1.0.0",
		"antlr4": "^4.10.1",
		"axios": "^0.21.2",
		"browserfs": "^1.4.3",
		"ckeditor4": "^4.18.0",
		"core-js": "^3.6.5",
		"d3": "^7.4.4",
		"grapesjs": "^0.18.4",
		"grapesjs-plugin-ckeditor": "0.0.10",
		"grapesjs-preset-webpage": "^0.1.11",
		"hcl": "github:ditrit/iactor#414a628ddb4e5b7ab761edbc0e5b1874ca64378a",
		"isomorphic-git": "^1.12.1",
		"mitt": "^3.0.0",
		"monaco-editor": "^0.25.2",
		"quasar": "^2.0.4",
		"three": "^0.130.1",
		"uuid": "^8.3.2",
		"vue": "^3.2.33",
		"vue-i18n": "^9.2.0-beta.22",
		"vue-router": "^4.0.15",
		"vuex": "^4.0.1",
		"vuex-persistedstate": "^4.0.0",
		"yaml-loader": "^0.6.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.13.14",
		"@quasar/app": "^3.3.3",
		"@quasar/cli": "^1.3.2",
		"@quasar/quasar-app-extension-testing": "^2.0.3",
		"@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-jest": "^25.2.2",
		"eslint-plugin-vue": "^7.0.0",
		"eslint-webpack-plugin": "^2.4.0",
		"jest-sonar-reporter": "^2.0.0",
		"json-server-auth": "^2.1.0",
		"sonar-scanner": "^3.1.0"
	},
	"browserslist": [
		"last 10 Chrome versions",
		"last 10 Firefox versions",
		"last 4 Edge versions",
		"last 7 Safari versions",
		"last 8 Android versions",
		"last 8 ChromeAndroid versions",
		"last 8 FirefoxAndroid versions",
		"last 10 iOS versions",
		"last 5 Opera versions"
	],
	"engines": {
		"node": ">= 12.22.1",
		"npm": ">= 6.13.4",
		"yarn": ">= 1.21.1"
	},
	"jestSonar": {
		"reportPath": "reports",
		"reportFile": "test-reporter.xml",
		"indent": 4
	}
}
